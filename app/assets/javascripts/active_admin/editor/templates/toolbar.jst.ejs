<div id="<%= id %>" class="toolbar">
  <ul>
    <li><a data-wysihtml5-action="change_view">Source</a></li>
    <li><a data-wysihtml5-command="bold" title="Bold">Bold</a></li>
    <li><a data-wysihtml5-command="italic" title="Italic">Italic</a></li>
    <li><a data-wysihtml5-command="createLink" title="Link">Link</a></li>
    <li><a data-wysihtml5-command="insertImage" class="insertImage" title="Image">Image</a></li>
    <li><a data-wysihtml5-command="insertUnorderedList" title="Unordered list">Bulleted List</a></li>
    <li><a data-wysihtml5-command="insertOrderedList" title="Ordered list">Numbered List</a></li>
    <li><a data-wysihtml5-command="indent" title="Indent">Indent</a></li>
    <li><a data-wysihtml5-command="outdent" title="Outdent">Outdent</a></li>
    <li><a data-wysihtml5-command="formatBlock" data-wysihtml5-command-value="h1">h1</a></li>
    <li><a data-wysihtml5-command="formatBlock" data-wysihtml5-command-value="h2">h2</a></li>
    <li><a data-wysihtml5-command="formatBlock" data-wysihtml5-command-value="h3">h3</a></li>
    <li><a data-wysihtml5-command="formatBlock" data-wysihtml5-command-value="h4">h4</a></li>
    <li><a data-wysihtml5-command="formatBlock" data-wysihtml5-command-value="h5">h5</a></li>
    <li><a data-wysihtml5-command="formatBlock" data-wysihtml5-command-value="blockquote">blockquote</a></li>    
    <li><a data-wysihtml5-command="insertHTML" data-wysihtml5-command-value="<div class='special-article-block special-block-location-center background'><div class='special-block-title'><h3>Title</h3></div><div class='special-article-block-inner'><p>Text</div></div>">Full incut</a></li>
    <li><a data-wysihtml5-command="insertHTML" data-wysihtml5-command-value="<div class='special-article-block special-block-location-left background'><div class='special-block-title'><h3>Title</h3></div><div class='special-article-block-inner'><p>Text</div></div>">Left incut</a></li>
    <li><a data-wysihtml5-command="insertHTML" data-wysihtml5-command-value="<div class='special-article-block special-block-location-right background'><div class='special-block-title'><h3>Title</h3></div><div class='special-article-block-inner'><p>Text</div></div>">Right incut</a></li>    
    <li><a data-wysihtml5-command="insertHTML" data-wysihtml5-command-value="<img src='/assets/flood/image_125.jpg' class='image-location-left' alt=''>'></a></li>
    <li><a data-wysihtml5-command="customClassInline" data-wysihtml5-command-value="bbb">customClassInline</a></li>
  </ul>

  <div class="dialog" data-wysihtml5-dialog="createLink" style="display: none;">
    <div class="action-group">
      <a href="#" data-wysihtml5-dialog-action="save" class="button">OK</a>
      <a href="#" data-wysihtml5-dialog-action="cancel">Cancel</a>
    </div>
    <label>
      Link:
      <input class="uploadable" type="text" data-wysihtml5-dialog-field="href" value="http://">
    </label>
    <div style="clear: both;"></div>
  </div>

  <div class="dialog" data-wysihtml5-dialog="insertImage" style="display: none;">
    <div class="action-group">
      <a href="#" data-wysihtml5-dialog-action="save" class="button">OK</a>
      <a href="#" data-wysihtml5-dialog-action="cancel">Cancel</a>
    </div>
    <label>
      Image URL:
      <input class="uploadable" type="text" data-wysihtml5-dialog-field="src" value="http://" id="image_url">
    </label>
    <label>
      Align:
      <select data-wysihtml5-dialog-field="className">
        <option value="">default</option>
        <option value="image-location-center">full center</option>
        <option value="image-location-left">half left</option>
        <option value="image-location-right">half right</option>
        <!-- <option value="wysiwyg-float-left">left</option>
        <option value="wysiwyg-float-right">right</option> -->
      </select>
    </label>
    <div style="clear: both;"></div>
    <div class="dialog-gallery"></div>
    <div style="clear: both;"></div>
  </div>
  
</div>
